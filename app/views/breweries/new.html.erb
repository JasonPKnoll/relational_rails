<form action="/breweries" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter a new brewery:</p>
  <input type='text' name='brewery[name]'/><br/>
  <p>Enter the year the brewery was established:</p>
  <input type='text' name='brewery[year]'/><br/>
  <input type='submit'/>
</form>
